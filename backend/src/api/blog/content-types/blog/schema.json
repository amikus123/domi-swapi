{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "blog.text",
        "blog.image",
        "blog.description-image",
        "blog.separator",
        "blog.related-blog"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "date": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "mainImage": {
      "type": "component",
      "repeatable": false,
      "component": "blog.description-image"
    },
    "blogCategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-category.blog-category",
      "inversedBy": "blogs"
    },
    "cardData": {
      "type": "component",
      "repeatable": false,
      "component": "blog.description-image"
    }
  }
}
