{
  "kind": "collectionType",
  "collectionName": "dishes",
  "info": {
    "singularName": "dish",
    "pluralName": "dishes",
    "displayName": "dish",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "nutrients": {
      "displayName": "nameAmount",
      "type": "component",
      "repeatable": true,
      "component": "dish.name-amount",
      "required": true
    },
    "ingredients": {
      "displayName": "ingredient",
      "type": "component",
      "repeatable": true,
      "component": "dish.ingredient",
      "required": true
    },
    "dishPage": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dish-page.dish-page"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "meal": {
      "type": "enumeration",
      "enum": [
        "obiad",
        "sniadanie"
      ],
      "default": "sniadanie",
      "required": true
    }
  }
}
